	F.ODBC.Connection!Conx.Execute("CREATE TABLE GAB_6370_MATREQ_HEAD (ID IDENTITY NOT NULL, DATE_REQUEST DATETIME, USER_REQUEST VARCHAR(8), EMPLOYEE VARCHAR(5), LOCATION INTEGER, DATE_UPDATE DATETIME, PRIMARY KEY (ID));")
F.Intrinsic.Control.EndIf

F.ODBC.Connection!conx.TableExists("GAB_6370_MATREQ_LINE",V.Local.bExist)
F.Intrinsic.Control.If(V.Local.bExist,=,False)
	F.ODBC.Connection!Conx.Execute("CREATE TABLE GAB_6370_MATREQ_LINE (REQ_ID INTEGER, REQ_SEQ INTEGER, JOB VARCHAR(6), SUFFIX VARCHAR(3), SEQ VARCHAR(6), PART VARCHAR(20), LOC VARCHAR(2), CUSTOMER_PO VARCHAR(20), QTY_REQ DOUBLE, QTY_ISSUED DOUBLE, LAST_ISSUED DATETIME, STATUS INTEGER, PRIMARY KEY (REQ_ID, REQ_SEQ));")
F.Intrinsic.Control.EndIf

F.ODBC.Connection!conx.TableExists("GAB_6370_SFDC_AREA",V.Local.bExist)
